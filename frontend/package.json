{
  "name": "codedswitch-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/ && node -e \"const fs=require('fs'); fs.copyFileSync('public/_redirects', 'dist/_redirects'); fs.copyFileSync('static.json', 'dist/static.json');\"",
    "build:render": "node render-build.js",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "prepare": "if [ \"$NODE_ENV\" != 'production' ]; then husky install || true; else echo 'Skipping husky install in production'; fi",
    "postinstall": "if [ \"$NODE_ENV\" = 'production' ]; then echo 'Skipping husky in production'; else npm run prepare; fi",
    "analyze": "cross-env ANALYZE=true vite build",
    "deploy": "npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@mui/icons-material": "^5.14.10",
    "@mui/material": "^5.14.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "axios": "^1.5.0",
    "formik": "^2.4.3",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.15.0",
    "react-syntax-highlighter": "^15.5.0",
    "tone": "^14.7.77",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "esbuild": "^0.18.20",
    "terser": "^5.43.1",
    "vite": "^4.4.9"
  }
}
